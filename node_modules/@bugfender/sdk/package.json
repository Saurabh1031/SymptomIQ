{
  "name": "@bugfender/sdk",
  "author": "Bugfender Team",
  "description": "Bugfender SDK for web, a remote logger tailor-made for apps",
  "homepage": "https://js.bugfender.com",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/mobilejazz/remote-logger-js.git"
  },
  "keywords": [
    "Bugfender",
    "remote logger",
    "logger",
    "debug"
  ],
  "license": "Apache-2.0",
  "version": "2.2.4",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "npm run build:embed && npm run build:lib",
    "build:embed": "gulp embed",
    "build:lib": "gulp lib",
    "docs": "typedoc --options typedoc.json",
    "docs:watch": "typedoc --watch --options typedoc.json",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:embed": "npm run build:embed && eslint --no-eslintrc --config=.eslintrc-compat.json build/bugfender-v2.js",
    "test:ci": "npm run test && npm run test:karma:ci",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:integration": "jest integration",
    "test:integration:watch": "jest integration --watch",
    "test:unit": "jest unit",
    "test:unit:watch": "jest unit --watch",
    "test:coverage": "jest unit --no-cache --collectCoverage",
    "test:karma": "karma start --browsers Chrome,Firefox,Webkit",
    "test:karma:ci": "karma start --single-run --browsers ChromeHeadlessNoSandbox,FirefoxHeadless,WebkitHeadless",
    "prepare": "npm run build:lib",
    "prepublishOnly": "npm run test:ci",
    "build:lib:watch": "gulp lib:watch",
    "preversion": "./bin/check-release-branch.sh",
    "serve:docs-dev": "hads ./docs-dev -o -i assets",
    "serve:prod": "gulp serve --env=prod",
    "serve:dev": "gulp serve --env=dev"
  },
  "dependencies": {
    "@bugfender/common": "^1.2.0",
    "base-58": "0.0.1",
    "bowser": "^2.11.0",
    "broadcast-channel": "^4.20.2",
    "error-stack-parser": "^2.1.4",
    "idb": "^7.1.1",
    "js-sha256": "^0.9.0",
    "stack-generator": "^2.0.10"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/gulp": "^4.0.10",
    "@types/gulp-connect": "^5.0.5",
    "@types/gulp-replace": "0.0.31",
    "@types/gulp-tap": "^1.0.1",
    "@types/gulp-uglify": "^3.0.7",
    "@types/jest": "^29.4.0",
    "@types/karma": "^6.3.3",
    "@types/minimist": "^1.2.2",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-stream": "^3.2.12",
    "@types/worker-plugin": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "compare-versions": "^5.0.3",
    "css-loader": "^6.7.3",
    "del": "^6.1.1",
    "eslint": "^8.35.0",
    "eslint-plugin-compat": "^4.1.2",
    "expect": "^29.5.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "glob": "^9.2.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-connect": "^5.7.0",
    "gulp-replace": "^1.1.4",
    "gulp-tap": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "hads": "^3.0.3",
    "html-loader": "^4.2.0",
    "jest": "^29.5.0",
    "jest-mock": "^29.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^5.5.3",
    "karma-typescript-es6-transform": "^5.5.3",
    "karma-webkit-launcher": "^2.1.0",
    "karma-webpack": "github:ryanclark/karma-webpack#master",
    "minimist": "^1.2.8",
    "playwright": "^1.31.2",
    "process": "^0.11.10",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.26",
    "typedoc-plugin-merge-modules": "^4.0.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-stream": "^7.0.0",
    "worker-loader": "^3.0.8"
  }
}
